{
  "scripts": {
    "dev": "vite-node --watch src/index.ts --mode=development",
    "dev:prod": "vite-node --watch src/index.ts --mode=production",
    "build": "vite build",
    "build:prod": "vite build --mode=production",
    "start": "vite-node src/index.ts",
    "start:prod": "vite-node src/index.ts --mode=production",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.0",
    "prettier-eslint-cli": "^8.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-node": "^3.2.4"
  }
}
